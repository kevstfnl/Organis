model Event {
    id          Int      @id @default(autoincrement())
    title       String
    start   DateTime
    end     DateTime

    entrepriseId Int
    entreprise   Enterprise @relation(fields: [entrepriseId], references: [id])

    userId Int
    user   User @relation(fields: [userId], references: [id])

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
