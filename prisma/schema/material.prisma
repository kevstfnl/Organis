model Material {
    id        Int        @id @default(autoincrement())
    type MaterialType
    description String
    mac String

    userId Int? @unique
    user User? @relation(fields: [userId], references: [id])

    enterpriseId Int
    enterprise  Enterprise @relation(fields: [enterpriseId], references: [id])

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
enum MaterialType {
    PHONE
    COMPUTER
    TABLET
}
