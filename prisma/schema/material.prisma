model Material {
    id        Int        @id @default(autoincrement())
    type Type
    description String
    mac String

    userId Int @unique
    user User @relation(fields: [userId], references: [id])

    enterpriseId Int
    enterprise  Enterprise @relation(fields: [enterpriseId], references: [id])

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
enum Type {
    PHONE
    COMPUTER
    TABLET
}
